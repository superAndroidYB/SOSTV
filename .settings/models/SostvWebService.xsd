<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.sostvapp.com/ws" xmlns="http://www.sostvapp.com/ws" elementFormDefault="qualified">
	<xs:element name="SostvRequest">
		<xs:complexType>
			<xs:all>
				<xs:element name="serviceType" type="xs:string"></xs:element>
				<xs:element name="source" type="xs:string"></xs:element>
				<xs:element name="data" type="xs:string"></xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="SostvResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="source" type="xs:string"></xs:element>
				<xs:element name="returnCode" type="xs:string"></xs:element>
				<xs:element name="returnMessage" type="xs:string"></xs:element>
				<xs:element name="returnData" type="xs:string"></xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>